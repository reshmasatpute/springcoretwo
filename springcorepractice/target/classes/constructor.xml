<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.technoelevate.springcorepractice.User"
		name="constructor">
		<constructor-arg value="Kiran" />
		<constructor-arg value="1234" />
		<constructor-arg value="9876543210" />
		<constructor-arg value="kiran@gmail.com" />
		<constructor-arg ref="insta" />
	</bean>
	<bean class="com.technoelevate.springcorepractice.WhatsApp"
		name="whatsapp" />
	<bean class="com.technoelevate.springcorepractice.Facebook"
		name="facebook" />
	<bean class="com.technoelevate.springcorepractice.Instagram"
		name="insta" />
</beans>